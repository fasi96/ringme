{
  "expo": {
    "name": "RingMe",
    "slug": "ringme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.fasiurrehman.ringme",
      "permissions": [
        "VIBRATE",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/alarm.wav"
          ]
        }
      ],
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
      }]
    ],
    "extra": {
      "eas": {
        "projectId": "49a5f97c-8995-40c0-ab0c-a0ddf7dd2b03"
      }
    },
    "owner": "fasiurrehman"
  }
}
